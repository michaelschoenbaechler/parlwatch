    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.0.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.3",
    "prettier": "^3.2.2",
    "ts-node": "~10.9.2",
    "typescript": "^5.8.3"
