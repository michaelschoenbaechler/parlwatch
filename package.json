    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/angular": "^7.6.4",
    "@ionic/storage-angular": "^4.0.0",
    "@jsverse/transloco": "^7.6.1",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/operators": "^20.0.0",
    "@ngrx/signals": "^20.0.0",
    "ionicons": "^7.2.2",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.1",
    "swissparl": "^0.6.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.3",
    "@angular-eslint/builder": "~20.1.1",
    "@angular-eslint/eslint-plugin": "~20.1.1",
    "@angular-eslint/eslint-plugin-template": "~20.1.1",
    "@angular-eslint/template-parser": "~20.1.1",
    "@angular/cli": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@angular/language-service": "^20.1.3",
    "@capacitor/cli": "^6.0.0",
    "@commitlint/cli": "^18.4.4",
