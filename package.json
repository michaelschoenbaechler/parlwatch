{
  "name": "parlwatch",
  "version": "1.0.3",
  "author": "Michael Schönbächler",
  "homepage": "https://github.com/michaelschoenbaechler",
  "description": "Easy access for everyone to parliamentary data",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prd": "ng build --configuration=production",
    "lint": "ng lint && npx prettier -c \"src/**/*.ts\"",
    "lint:fix": "ng lint --fix",
    "prettier:fix": "npx prettier --write \"src/**/*.ts\"",
    "i18n:extract": "transloco-keys-manager extract --remove-extra-keys --sort",
    "i18n:find": "transloco-keys-manager find",
    "android:sync": "npx cap sync android",
    "android:build": "npm run build:prd && npx cap sync android",
    "android:open": "npx cap open android",
    "ios:sync": "npx cap sync ios",
    "ios:build": "npm run build:prd && npx cap sync ios",
    "ios:open": "npx cap open ios"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^18.2.5",
    "@angular/core": "^18.2.5",
    "@angular/forms": "^18.2.5",
    "@angular/platform-browser": "^18.2.5",
    "@angular/platform-browser-dynamic": "^18.2.5",
    "@angular/router": "^18.2.5",
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/angular": "^7.6.4",
    "@ionic/storage-angular": "^4.0.0",
    "@jsverse/transloco": "^7.6.1",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/operators": "^18.0.2",
    "@ngrx/signals": "^18.0.2",
    "ionicons": "^7.2.2",
    "lodash": "^4.17.21",
    "rxjs": "~7.8.1",
    "swissparl": "^0.6.2",
    "tslib": "^2.2.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular-eslint/builder": "~18.3.1",
    "@angular-eslint/eslint-plugin": "~18.3.1",
    "@angular-eslint/eslint-plugin-template": "~18.3.1",
    "@angular-eslint/template-parser": "~18.3.1",
    "@angular/cli": "^18.2.5",
    "@angular/compiler": "^18.2.5",
    "@angular/compiler-cli": "^18.2.5",
    "@angular/language-service": "^18.2.5",
    "@capacitor/cli": "^6.0.0",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@ionic/angular-toolkit": "^12.1.1",
    "@jsverse/transloco-keys-manager": "^5.1.0",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.11.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.0.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.2.2",
    "ts-node": "~10.9.2",
    "typescript": "^5.2.0"
  }
}
